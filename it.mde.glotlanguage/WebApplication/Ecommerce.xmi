<?xml version="1.0" encoding="UTF-8"?>
<web:System
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:dc="https://it.mde.glotlanguage/dataConcern"
    xmlns:web="https://it.mde.glotlanguage"
    xsi:schemaLocation="https://it.mde.glotlanguage/dataConcern https://it.mde.glotlanguage/formConcern#//DataConcern https://it.mde.glotlanguage https://it.mde.glotlanguage/formConcern"
    name="Ecommerce"
    version="1"
    subversion="1">
  <entities
      name="Post">
    <features
        xsi:type="dc:Attribute"
        name="id"
        isPrimaryKey="true"
        type="int"/>
    <features
        xsi:type="dc:Attribute"
        name="title"/>
  </entities>
  <pages name="Posts">
    <indexes
        name="Posts"
        entity="//@entities.0"
        features="//@entities.0/@features.0 //@entities.0/@features.1">
      <individuals
          name="Post"
          entity="//@entities.0"
          features="//@entities.0/@features.0 //@entities.0/@features.1"/>
    </indexes>
  </pages>
</web:System>
